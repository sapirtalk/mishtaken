{
  "manifest_version": 2,
  "name": "dira tracker",
  "version": "1.3.20",
  "permissions": [
    "notifications",
    "https://www.dira.moch.gov.il/ProjectsList"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "home-button.png",
    "48": "home-button.png",
    "128": "home-button.png"
  }
}



