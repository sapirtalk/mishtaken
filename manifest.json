{
  "manifest_version": 3,
  "name": "dira tracker",
  "version": "1.3.215",
  "permissions": [
    "scripting",
    "notifications",
    "storage",
    "background"
  ],
  "host_permissions": [
    "https://www.dira.moch.gov.il/ProjectsList"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}




